@model IEnumerable<NewspaperSubscription.Models.Subscription>

@{
    ViewData["Title"] = "GetAllSubscriptions";
}

<h1>GetAllSubscriptions</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Subscriptiontype)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subscriptionstartdate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NewspaperNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subscriptiontype)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subscriptionstartdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerNavigation.Customername)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NewspaperNavigation.Newspapername)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorNavigation.Vendorname)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Subscriptionid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Subscriptionid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
